		</dependency>
		<dependency>
			<groupId>org.jmockit</groupId>
			<artifactId>jmockit</artifactId>
			<version>1.12</version>
		</dependency>
		<!--
			Get the following jar from https://sourceforge.net/projects/jpasecurity/files/jpasecurity/jpasecurity-0.4.0/.
			And add to local repository as below:
			$ mvn install:install-file -Dfile=jpasecurity-core-0.4.0.jar -DgroupId=net.sf.jpasecurity -DartifactId=jpasecurity-core -Dversion=0.4.0 -Dpackaging=jar
		-->
		<dependency>
			<groupId>net.sf.jpasecurity</groupId>
			<artifactId>jpasecurity-core</artifactId>
			<version>0.4.0</version>
		</dependency>
		<!--
			Get the following jar https://sourceforge.net/projects/sblim/files/sblim-cim-client/1.3.9.1/sblim-cim-client-1.3.9.1-bin.zip/download
			And add to local repository as below:
			$ mvn install:install-file -Dfile=sblim-cim-client-1.3.9.1/cim-client/sblimCIMClient.jar -DgroupId=org.slbim.wbem -DartifactId=CIMClient -Dversion=1.3.9.1 -Dpackaging=jar
		-->
		<dependency>
			<groupId>org.slbim.wbem</groupId>
			<artifactId>CIMClient</artifactId>
			<version>1.3.9.1</version>
		</dependency>
		<!-- Customized third party components -->
		<!-- https://software.intel.com/en-us/articles/download-the-latest-intel-ws-management-java-client-library  -->
		<dependency>
			<groupId>hinemos</groupId>
			<artifactId>intel-wsman</artifactId>
			<version>1.0.1</version>
		</dependency>
		<!-- http://www.snmp4j.org/ -->
		<dependency>
			<groupId>hinemos</groupId>
			<artifactId>snmp4j</artifactId>
			<version>2.3.1</version>
		</dependency>
	</dependencies>
</project>