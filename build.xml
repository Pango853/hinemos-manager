		<macrodef name="wsgen.mac">
			<attribute name="sei.class" />
			<sequential>
				<wsgen
					fork="false"
					verbose="true"
					destdir="${wsdlsrc.dir}"
					resourcedestdir="${wsdl.dir}"
					keep="false"
					genwsdl="true"
					sei="@{sei.class}">
